{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/levitarmouse/KISS-ORM"
        },
        {
            "type": "git",
            "url": "https://github.com/levitarmouse/KISS-CORE"
        }
    ],
    "name": "levitarmouse/kiss_orm_demo",
    "description": "Active record ORM demo",
    "license": "proprietary",
    "type": "project",
    "require": {
        "levitarmouse/kiss_orm": "dev-1.0-dev",
        "levitarmouse/core": "1.1.4"
    },
    "authors": [
        {
            "name": "Gabriel Prieto",
            "email": "levitarmouse@gmail.com"
        }
    ],
    "scripts": {
        "post-update-cmd": ["php -r \"shell_exec('mkdir -p config/kissorm');\"",
                             "php -r \"shell_exec('ln -s ./vendor/levitarmouse/kiss_orm/tables.ini .');\"",
                             "php -r \"shell_exec('ln -s ./vendor/levitarmouse/kiss_orm/kissGen.php .');\"",
                             "php -r \"shell_exec('cd config/kissorm; ln -s ../../vendor/levitarmouse/kiss_orm/config/config.php .');\"",
                             "php -r \"shell_exec('cd config/kissorm; ln -s ../../vendor/levitarmouse/kiss_orm/config/database.ini .');\"",
                             ""],
        "post-package-install": ["php -r \"shell_exec('mkdir -p config/');\"",
                             "php -r \"shell_exec('ln -s ./vendor/levitarmouse/kiss_orm/tables.ini .');\"",
                             "php -r \"shell_exec('ln -s ./vendor/levitarmouse/kiss_orm/kissGen.php .');\"",
                             "php -r \"shell_exec('cd config/kissorm; ln -s ../../vendor/levitarmouse/kiss_orm/config/config.php .');\"",
                             "php -r \"shell_exec('cd config/kissorm; ln -s ../../vendor/levitarmouse/kiss_orm/config/database.ini .');\"",
                             ""],
        "post-install-cmd": ["php -r \"shell_exec('mkdir -p config/');\"",
                             "php -r \"shell_exec('ln -s ./vendor/levitarmouse/kiss_orm/tables.ini .');\"",
                             "php -r \"shell_exec('ln -s ./vendor/levitarmouse/kiss_orm/kissGen.php .');\"",
                             "php -r \"shell_exec('cd config/kissorm; ln -s ../../vendor/levitarmouse/kiss_orm/config/config.php .');\"",
                             "php -r \"shell_exec('cd config/kissorm; ln -s ../../vendor/levitarmouse/kiss_orm/config/database.ini .');\"",
                             ""],
        "post-autoload-dump": [],
        "post-create-project-cmd": []
    }
}